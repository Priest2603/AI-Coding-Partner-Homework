{
  "name": "intelligent-customer-support-system",
  "version": "1.0.0",
  "description": "Customer support ticket management system with multi-format import and auto-classification",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:load": "ts-node tests/load/concurrent.load.ts",
    "test:all": "npm run test && npm run test:load",
    "lint": "eslint . --ext .ts"
  },
  "keywords": ["customer-support", "ticket-management", "express", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^5.2.1",
    "zod": "^3.25.76",
    "multer": "^2.0.2",
    "csv-parse": "^6.1.0",
    "fast-xml-parser": "^5.3.4",
    "cors": "^2.8.5",
    "winston": "^3.14.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.6",
    "@types/multer": "^2.0.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.7.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "supertest": "^7.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "autocannon": "^7.15.0",
    "@types/autocannon": "^7.12.6",
    "concurrently": "^9.1.2"
  }
}
